    "next-themes": "^0.4.6",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "resend": "^6.8.0",
    "sanity": "^4.22.0",
    "server-only": "^0.0.1",
    "styled-components": "^6.3.8",
    "superjson": "^2.2.1",
    "tailwind-merge": "^3.4.0",
